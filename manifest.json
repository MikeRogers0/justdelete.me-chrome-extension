{
  "name": "__MSG_name__",
  "version": "1.3.3",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "default_locale": "en",
  "background": {
    "scripts": ["js/background.js"]
  },
  "homepage_url": "http://justdelete.me/",
  "default_locale": "en",
  "permissions": [
    "tabs", "<all_urls>"
  ],
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "page_action": {
    "default_icon": { 
      "38": "img/icon_easy_38.png" 
    },
    "default_title": "JustDelete.me"
  },
  "minimum_chrome_version": "18",
  "offline_enabled": false
}